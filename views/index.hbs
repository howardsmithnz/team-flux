<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<form class="form-horizontal">
<fieldset>

<!-- Form Name -->
<legend>Form Name</legend>

<!-- Text input-->
<div class="form-group">
  <label class="col-md-4 control-label" for="textinput">Query Text</label>  
  <div class="col-md-4">
  <input id="textinput" name="textinput" type="text" placeholder="enter your text here" class="form-control input-md" required="">
    
  </div>
</div>

<!-- Button -->
<div class="form-group" id="push_me">
  <label class="col-md-4 control-label" for="sendbutton">Send Query</label>
  <div class="col-md-4">
    <button id="sendbutton" name="sendbutton" class="btn btn-primary">Send</button>
  </div>
</div>

</fieldset>
</form>

<script>
    $("#push_me").click(function(){
    $.get("http://snomed.info/sct&code=263495000&_format=json", function(data, status){
        alert("Data: " + data + "\nStatus: " + status);
    });
});
</script>